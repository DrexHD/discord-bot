{
  "name": "modrunner",
  "version": "1.4.10",
  "private": true,
  "main": "bot.js",
  "scripts": {
    "setup-workspace": "npm install",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "eslint": "eslint .",
    "eslint-fix": "eslint fix .",
    "build": "npm run register-globally && npm run alter-db",
    "start": "node .",
    "dev": "nodemon .",
    "register": "node scripts/register-commands.js",
    "register-globally": "node scripts/register-commands.js --global",
    "create-db": "node scripts/initialize-database.js",
    "reset-db": "node scripts/initialize-database.js --force",
    "alter-db": "node scripts/initialize-database.js --alter",
    "migrate": "node scripts/migrate-database.js",
    "set-guild-max": "node scripts/set-guild-max-tracking.js"
  },
  "dependencies": {
    "@discordjs/rest": "^1.6.0",
    "@logtail/pino": "^0.5.2",
    "dayjs": "^1.11.7",
    "discord-api-types": "^0.37.37",
    "discord.js": "^14.8.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "ms": "^2.1.3",
    "openai": "^4.76.0",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "sequelize": "^6.30.0",
    "sqlite3": "^5.1.7",
    "undici": "^5.21.0"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^3.1.7",
    "prettier": "^2.8.7"
  }
}
